<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item>
    <shape android:shape="rectangle">
        <corners android:radius="1dp" />
        <stroke
            android:width="2dp"
            android:color="#616161" />
    </shape>
</item>
<item
    android:right="2dp"
    android:left="2dp">
    <shape android:shape="rectangle">
        <solid android:color="#795548" />
    </shape>
</item>
</layer-list>